@{
    ViewBag.Title = "Index";
}

<div class="card">
    <div class="card-header">
        <h2 class="card-title">Manage Employee</h2>
        <div class="card-tools">
            <a href="@Url.Action("Manage","Employee")" class="btn btn-sm btn-primary"> Add New</a>
        </div>
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table id="tblCategory" class="table table-striped table-bordered table-hover table-sm">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Desc</th>
                        <th style="width:1px">Edit</th>
                        <th style="width:1px">Delete</th>
                    </tr>
                </thead>
                <tbody>
                    <tr class="odd"><td valign="top" colspan="10" class="text-center">No results</td></tr>
                </tbody>
            </table>
        </div>
    </div>
</div>

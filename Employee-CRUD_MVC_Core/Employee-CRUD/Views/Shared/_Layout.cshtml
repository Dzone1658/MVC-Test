<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>@ViewBag.Title</title>

    <link rel="shortcut icon" type="image/png" href="/favicon.png" />
    <link rel="shortcut icon" type="image/png" href="~/Assets/dist/img/neoparx.jpg" />

    <!-- Font Awesome Icons -->
    <link href="@("/")Assets/Plugins/fontawesome-free/css/all.min.css" rel="stylesheet" />
    <!-- Theme style -->
    <link href="@("/")Assets/dist/css/adminlte.min.css" rel="stylesheet" />
    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
    <link href="~/Assets/Plugins/toastr/toastr.css" rel="stylesheet" />
    <link href="~/Assets/Plugins/sweetalert2/sweetalert2.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/flatpickr/4.6.3/flatpickr.min.css" />
    <link href="~/Assets/Plugins/Select2/select2.min.css" rel="stylesheet" />
    <link href="~/Assets/Plugins/datatables-bs4/css/dataTables.bootstrap4.min.css" rel="stylesheet" />

</head>
<body class="hold-transition layout-top-nav">
    <div class="wrapper">
        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand-md navbar-light navbar-white">
            <div class="container">
                <a href="../../index3.html" class="navbar-brand">
                    <img src="@("/")Assets/dist/img/neoparx.jpg" alt="AdminLTE Logo" class="brand-image img-circle elevation-3" style="opacity: .8">
                    <span class="brand-text font-weight-light">AdminLTE 3</span>
                </a>

                <button class="navbar-toggler order-1" type="button" data-toggle="collapse" data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse order-3" id="navbarCollapse">
                    <!-- Left navbar links -->
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a href="Home" class="nav-link">Home</a>
                        </li>
                        <li class="nav-item">
                            <a href="Quotes" class="nav-link">Quotes</a>
                        </li>
                    </ul>

                    <!-- SEARCH FORM -->
                    <form class="form-inline ml-0 ml-md-3">
                        <div class="input-group input-group-sm">
                            <input class="form-control form-control-navbar" type="search" placeholder="Search" aria-label="Search">
                            <div class="input-group-append">
                                <button class="btn btn-navbar" type="submit">
                                    <i class="fas fa-search"></i>
                                </button>
                            </div>
                        </div>
                    </form>
                </div>

                <!-- Right navbar links -->
                <ul class="order-1 order-md-3 navbar-nav navbar-no-expand ml-auto">
                    UserName
                    <!-- Notifications Dropdown Menu -->
                    <li class="nav-item dropdown">
                        <a class="nav-link" data-toggle="dropdown" href="#">
                            <i class="far fa-user"></i>
                        </a>
                        <div class="dropdown-menu dropdown-menu-lg dropdown-menu-right">
                            <a href="#" class="dropdown-item">
                                My Profile
                            </a>
                            <div class="dropdown-divider"></div>
                            <a href="#" class="dropdown-item">
                                Change Password
                            </a>
                            <div class="dropdown-divider"></div>
                            <a href="@Url.Action("Logout","Account")" class="dropdown-item">
                                @*<i class="fa fa-key mr-2"></i>*@ Logout
                            </a>
                        </div>
                    </li>

                </ul>
            </div>
        </nav>
        <!-- /.navbar -->
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Side menu -->
            <a href="@Url.Action("Index","Home")" class="brand-link">
                <img src="@("/")Assets/dist/img/neoparx.jpg" alt="AdminLTE Logo" class="brand-image img-circle elevation-3"
                     style="opacity: .8">
                <span class="brand-text font-weight-light">Quotes Application</span>
            </a>
        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Main content -->
            <div class="content">
                <div class="pt-3"></div>
                <div class="container">
                    @RenderBody()
                </div><!-- /.container-fluid -->
            </div>
            <!-- /.content -->
        </div>
        <!-- Main Footer -->
        <footer class="main-footer">
            <!-- To the right -->
            <div class="d-none d-sm-inline">
                Copyright © <b>Quotes Appplication</b> All rights reserved
            </div>
            <!-- Default to the left -->
        </footer>
    </div>

    <script src="@("/")Assets/Plugins/jquery/jquery.min.js"></script>
    <script src="@("/")Assets/Plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="@("/")Assets/dist/js/adminlte.min.js"></script>
    <script src="@("/")Assets/Plugins/jquery-ui/jquery-ui.min.js"></script>
    <script src="@("/")Assets/Plugins/jquery-validation/jquery.validate.js"></script>
    <script src="@("/")Assets/Plugins/jquery-validation/jquery.validate.unobtrusive.js"></script>
    <script src="@("/")Assets/dist/js/bootstrap-datepicker.min.js"></script>
    <script src="@("/")Assets/Plugins/sweetalert2/sweetalert2.min.js"></script>
    <script src="@("/")Assets/Plugins/toastr/toastr.min.js"></script>
    <script src="@("/")Assets/Plugins/datatables/jquery.dataTables.min.js"></script>
    <script src="@("/")Assets/Plugins/Select2/select2.min.js"></script>
    <script src="~/js/common.js"></script>
    @*@Scripts.Render("~/Scripts/Custom")*@

    @RenderSection("scripts", required: false)
    <script type="text/javascript">
        var current = location.pathname;
        $('.list-group li a').removeClass('active');
        $('.list-group li a').each(function () {
            if ($(this).data("name") == current) {
                $(this).addClass('active');
            }
        });
    </script>
</body>
</html>




